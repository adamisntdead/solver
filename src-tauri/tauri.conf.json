{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Poker Solver",
  "identifier": "com.pokersolver.desktop",
  "version": "0.1.0",
  "build": {
    "frontendDist": "../webapp"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Poker Solver",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png"
    ]
  }
}
